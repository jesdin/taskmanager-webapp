<script src="https://code.jquery.com/jquery-1.11.0.min.js"></script>
<script type="text/javascript">
    function sendPutRequest(id, isCompleted) {
        var text
        if (isCompleted == 'True') {
            text = "SetAsIncomplete/"
        }
        else {
            text = "SetAsComplete/"
        }
        
        $.ajax({
            type: "post",
            url: "/api/MyTasks/" + text + id,
            contentType: "application/json",
            data: id,
            success: function () {
                console.log(id);
                location.reload();
            }
        })
    }
</script>