<script src="https://code.jquery.com/jquery-1.11.0.min.js"></script>
<script type="text/javascript">
    function sendPutRequest(id, title, desc, priority, isCompleted) {
        if (isCompleted == 'True') {
            isCompleted = 'false';
        }
        else {
            isCompleted = 'true';
        }
        
        $.ajax({
            type: "put",
            url: "/api/MyTasks/" + id,
            contentType: "application/json",
            data: JSON.stringify({
                "id": id,
                "title": title,
                "description": desc,
                "isCompleted": isCompleted,
                "isHighPriority": priority.toLowerCase()
            }),
            success: function () {
                console.log(id);
                location.reload();
            }
        })
    }
</script>